<!DOCTYPE HTML>
<html lang="<%= config.language %>">
<%- partial('_partial/head') %>

<body>

<%- partial('_partial/header') %>
<%- body %>
<%- partial('_partial/footer') %>

<%- partial('_partial/search') %>
<%- partial('_partial/back-top') %>

<script src="<%- theme.libs.js.materialize %>"></script>
<script src="<%- theme.libs.js.masonry %>"></script>
<script src="<%- theme.libs.js.aos %>"></script>
<script src="<%- theme.libs.js.scrollProgress %>"></script>
<script src="<%- theme.libs.js.lightgallery %>"></script>
<script src="/js/matery.js"></script>

<script src="/libs/device/current-device.js"></script>

<!-- 谷歌分析 -->
<%- partial('_partial/google-analytics') %>

<!-- 页面点击小红心 -->
<% if (theme.clickLove.enable) { %>
    <script src="/libs/others/click-love.js"></script>
<% } %>

<!-- 不蒜子统计 -->
<% if (theme.busuanziStatistics && theme.busuanziStatistics.enable) { %>
    <script async src="/libs/others/busuanzi.pure.mini.js"></script>
<% } %>

<!-- 页面动态背景 -->
<% if (theme.canvasNest.enable) { %>
    <script>
        window.onload = function() {
            console.log("device.mobile() === %s", device.mobile());
            if(device.mobile()) {
	        return;
            }
	    var script = document.createElement("script");
	    script.setAttribute("type", "text/javascript");
	    script.src = "/libs/others/canvas-nest.min.js";
	    document.getElementsByTagName("head")[0].appendChild(script);
        }
    </script>
<% } %>

<!-- 本站运行时间 -->
<% if (theme.runTime.enable) { %>
    <script src="/libs/others/run-time.js"></script>
<% } %>

</body>
</html>